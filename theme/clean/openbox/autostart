#!/bin/sh

if command -v xrandr >/dev/null 2>&1 && [ -f "$HOME/.config/openbox/resolution.sh" ]; then
    . "$HOME/.config/openbox/resolution.sh"
fi

if command -v polybar >/dev/null 2>&1; then
    polybar main &
fi

if command -v picom >/dev/null 2>&1; then
    picom &
fi

if command -v nitrogen >/dev/null 2>&1; then
    nitrogen --restore &
fi
